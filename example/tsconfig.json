{
  "include": [".dumi/**/*", ".dumirc.ts", "docs"],
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "dumi/theme/*": [".dumi/tmp/dumi/theme/*"],
      "dumi-theme-antd-style": ["../src"]
    }
  }
}
