{
  "include": ["src", "tests", ".dumi/**/*", "example/.dumirc.ts", "*.ts"],
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "@/*": ["src"],
      "dumi/theme/*": ["./.dumi/tmp/dumi/theme/*"],
      "dumi-theme-antd-style": ["src"],
      "dumi-theme-antd-style/*": ["src/*", "*"]
    }
  }
}
